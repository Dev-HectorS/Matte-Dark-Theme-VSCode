{
	"name": "Matte Dark Theme",
	"type": "dark",
	"colors": {
		// Title bar 
		"titleBar.activeBackground": "#141414",
		
		// Buttons
		"button.background":"#82D305",
		"button.foreground": "#191919",

		// Seleccion 
		"focusBorder":"#82D305",
		
		// Lists and trees
		"list.activeSelectionBackground": "#81d30598",
		"list.activeSelectionForeground": "#191919",
		"list.dropBackground": "#a183d333",
		"list.errorForeground": "#E84947",
		"list.focusBackground": "#2D2D2D",
		"list.focusForeground": "#81d30598",

		// Command pallete
		"pickerGroup.foreground":"#82D305",
		"pickerGroup.border": "#82D305",

		// Activity bar
		"activityBar.background": "#141414",
		"activityBar.border": "#141414",

		// Activity bar badge
		"activityBarBadge.background": "#C8F902",
		"activityBarBadge.foreground": "#2D2D2D",

		// Side bar
		"sideBarTitle.foreground": "#97DC21",
		"sideBarSectionHeader.background": "#2D2D2D",
		"sideBarSectionHeader.foreground": "#97DC21",
		"sideBar.background": "#141414",
		"sideBar.foreground": "#A1A1A1",
		"sideBar.border": "#141414",

		// Progress bar loading
		"progressBar.background":"#82D305",

		// Extension button
		"extensionButton.prominentHoverBackground":"#81d30598",

		// Input 
		"input.border": "#82D305",
		"inputValidation.errorBackground":"#2D2D2D",
		"inputValidation.errorForeground":"#d5d5d0",
		"inputValidation.errorBorder":"#2D2D2D",
		"inputOption.activeBackground":"#141414",
		"inputOption.activeForeground":"#A1A1A1",
		"inputOption.activeBorder":"#141414",
		
		// Editor Groups & Tabs
		"editor.selectionBackground": "#2D2D2D",
		"editor.background": "#191919",
		"editor.foreground": "#BABABA",
		"editorGroup.emptyBackground":"#191919",
		"editorGroupHeader.tabsBackground": "#191919",
		"editorGutter.modifiedBackground": "#82D305",
		"editorWarning.foreground": "#5BD2F0",
		"editorError.foreground": "#FF6962",

		// Tab
		"tab.activeBackground":"#191919",
		"tab.activeBorder": "#82D305",
		"tab.inactiveBackground": "#2D2D2D",

		// Text colors
		"textLink.foreground": "#82D305",

		// Panel
		"panel.border":"#82D305",

		// Dropdown
		"dropdown.background":"#191919",
		"dropdown.border": "#82D305",
		
		// Status bar
		"statusBar.noFolderBackground": "#191919",
		"statusBar.background": "#2D2D2D",
		"statusBar.foreground": "#C8F902",
		"statusBar.border": "#2D2D2D",
		"statusBar.debuggingForeground":"#e9e7e7",
		"errorForeground":"#FF6962",

		// Git
		"gitDecoration.addedResourceForeground": "#82D305",
		"gitDecoration.modifiedResourceForeground": "#E27429",
		"gitDecoration.untrackedResourceForeground": "#F2F3F4",
		"gitDecoration.ignoredResourceForeground": "#32D0D1",
		"gitDecoration.conflictingResourceForeground": "#E84947",

		// Terminal
		"terminal.background": "#141414",
		"terminal.foreground": "#A1A1A1",
		"terminalCursor.background": "#A1A1A1",
		"terminalCursor.foreground": "#A1A1A1",
		"terminal.ansiBrightYellow": "#C8F902",
		"terminal.ansiCyan": "#C8F902",
		"terminal.ansiBrightBlack": "#F2F3F4",
		"terminal.ansiRed": "#E84947",
		"terminal.ansiBlue": "#ff1e48",
		"terminal.ansiBrightRed": "#ff1e48",
		"terminal.ansiGreen": "#5BD2F0",
		"terminal.ansiBrightGreen": "#5BD2F0",
		"terminal.ansiMagenta": "#FEF24E",
		"terminal.ansiYellow": "#FEF24E",
		"terminal.ansiBrightMagenta": "#FEF24E",
		"terminal.ansiWhite": "#d5d5d0",
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#6A6B6B"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#C8F902"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#f00028"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#BCBEBE"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.control",
				"constant.other.color",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#6A6B6B"
			}
		},
		// Operators
		{
			"name": "Operator, Misc",
			"scope": ["meta.tag"],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": ["punctuation"],
			"settings": {
				"foreground": "#BCBEBE"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#5BD2F0"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#5BD2F0"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other"
			],
			"settings": {
				"foreground": "#C8F902"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#e6c7c8"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#FF6962"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
